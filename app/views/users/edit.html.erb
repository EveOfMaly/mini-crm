<div>
Edit page

<%= form_for @user, :url => {action: "update"} do |f| %>
    <%= f.label :username %><br>
    <%= f.text_field :username %> <br>
    <%= f.label :email %><br>
    <%= f.text_field :email %> <br>
    <%= f.hidden_field :password, value: @user.password_digest %>
    <%= f.submit  %>
<% end %>
</div>

